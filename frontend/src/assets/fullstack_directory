│   │   │   └── index.css
│   │   ├── utils/                # 유틸 함수
│   │   │   └── dateUtils.ts
│   │   ├── App.tsx
│   │   └── main.tsx
│   ├── package.json
│   └── tsconfig.json
│
├── backend/                      # NestJS + Kysely + PostgreSQL
│   ├── src/
│   │   ├── app.module.ts
│   │   ├── main.ts
│   │   ├── db/                   # Kysely DB provider + 마이그레이션
│   │   │   ├── kysely.provider.ts
│   │   │   └── migrations.sql
│   │   ├── auth/                 # JWT 로그인/회원가입
│   │   │   ├── auth.module.ts
│   │   │   ├── auth.service.ts
│   │   │   └── auth.controller.ts
│   │   ├── users/                # 사용자 CRUD
│   │   │   ├── users.module.ts
│   │   │   ├── users.service.ts
│   │   │   └── users.controller.ts
│   │   ├── habits/               # 습관 CRUD
│   │   │   ├── habits.module.ts
│   │   │   ├── habits.service.ts
│   │   │   └── habits.controller.ts
│   │   └── habit-logs/           # 습관 기록 CRUD
│   │       ├── habit-logs.module.ts
│   │       ├── habit-logs.service.ts
│   │       └── habit-logs.controller.ts
│   ├── package.json
│   └── .env
│
├── .gitignore
